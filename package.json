{
  "name": "sketchel",
  "version": "0.0.1",
  "description": "A modern, community-driven, social drawing website.",
  "main": "server.js",
  "scripts": {
    "start": "npm run css-build && npm run dark-build && nodemon index.js",
    "dev": "npm run css-build && npm run dark-build && cross-env HTTPS=false npm run start",
    "css-build": "node-sass --omit-source-map-url public/scss/style.scss public/css/style.css",
    "css-watch": "npm run css-build -- --watch",
    "dark-build": "node-sass --omit-source-map-url public/scss/dark.scss public/css/dark.css",
    "dark-watch": "npm run dark-build -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sketchel/sketchel.git"
  },
  "keywords": [
    "drawing",
    "website",
    "social",
    "sketchel"
  ],
  "author": "Minota",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sketchel/sketchel/issues"
  },
  "homepage": "https://github.com/sketchel/sketchel#readme",
  "devDependencies": {
    "body-parser": "^1.19.0",
    "bulma": "^0.8.2",
    "cookie-parser": "^1.4.5",
    "date-and-time": "^0.11.1",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "node-sass": "^4.14.1",
    "nodemon": "^1.19.4",
    "path": "^0.12.7",
    "quick.db": "^7.1.2",
    "standard": "^14.3.4"
  },
  "dependencies": {
    "@sentry/node": "^5.24.2",
    "base64-to-image": "^1.0.2",
    "bcrypt": "^5.0.0",
    "bulma-checkradio": "^1.1.1",
    "bulma-divider": "^0.2.0",
    "bulma-switch": "^2.0.0",
    "bulma-tooltip": "^3.0.2",
    "canvas": "^2.6.1",
    "connect-flash": "^0.1.1",
    "cross-env": "^6.0.3",
    "datauri": "^2.0.0",
    "express-session": "^1.17.1",
    "helmet": "^3.23.3",
    "http": "0.0.1-security",
    "markdown": "^0.5.0",
    "opn": "^6.0.0",
    "passport": "^0.4.1",
    "pug": "^2.0.4",
    "session": "^0.1.0",
    "uuid": "^3.4.0",
    "xss": "^1.0.8",
    "xxhash": "^0.3.0"
  }
}
